https://onlinejudge.org/external/101/10106.pdf

#include <bits/stdc++.h>
using namespace std;

string multiplyStrings(string &a, string &b) {
    if (a == "0" || b == "0") return "0";

    int n = a.size(), m = b.size();
    vector<int> result(n + m, 0);  

    
    for (int i = n - 1; i >= 0; i--) {
        for (int j = m - 1; j >= 0; j--) {
            int mul = (a[i] - '0') * (b[j] - '0');
            int posLow = i + j + 1;   
            int posHigh = i + j;      
            mul += result[posLow];
            result[posLow] = mul % 10;
            result[posHigh] += mul / 10;
        }
    }
    string product;
    for (int num : result)
        if (!(product.empty() && num == 0))
            product.push_back(num + '0');
    return product.empty() ? "0" : product;
}

int main() {
    string A, B;
    while (cin >> A >> B) 
        cout << multiplyStrings(A, B) << "\n";
}
